{
  "name": "TSTemplate",
  "version": "1.0.0",
  "description": "Template for React App using TypeScript",
  "main": "index.js",
  "author": "ClintonBaker <c.dillon.baker@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "prebuild": "run-s clean",
    "build": "parcel build src/index.html --no-source-maps",
    "clean": "rimraf ./dist",
    "start": "link-module-alias && NODE_ENV=DEV parcel src/index.html --port 3000"
  },
  "devDependencies": {
    "@types/react": "^16.9.43",
    "@types/react-dom": "^16.9.8",
    "@typescript-eslint/eslint-plugin": "^3.7.1",
    "@typescript-eslint/parser": "^3.7.1",
    "eslint": "^7.5.0",
    "eslint-plugin-react": "^7.20.5",
    "eslint-plugin-react-hooks": "^4.0.8",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.4",
    "sass": "^1.26.10",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "@types/node": "^14.0.27",
    "config": "^3.3.1",
    "dotenv": "^8.2.0",
    "link-module-alias": "^1.2.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "_moduleAliases": {
    "~": "dist"
  },
  "_moduleAliasIgnoreWarning": true
}
